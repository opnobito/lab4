#include <stdio.h>

int main() {
    int sum = 0;
    for (int i = 12; i <= 20; i += 2) {
        sum += i;
    }

    int cubesum = sum * sum * sum;
    printf("Куб суми прних чисел = %d\n", cubesum);

    return 0;
}







#include <stdio.h>

int main() {
    int sum = 0, i = 12;
    while (i <= 20) { 
        sum += i;
        i += 2;
    }
    int cubesum = sum * sum * sum;
    printf("Куб суми прних чисел = %d\n", cubesum);

    return 0;
}







#include <stdio.h>

int main() {
    int sum = 0, i = 12;
    do {
        sum += i;
        i += 2;
    } while (i <= 20);
    int cubesum = sum * sum * sum;
    printf("Куб суми прних чисел = %d\n", cubesum);

    return 0;
}



