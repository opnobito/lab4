#include <stdio.h>

int main() {
    int sum = 0;
    for (int i = 12; i <= 20; i += 2) {
        sum += i;
    }

    int cubesum = sum * sum * sum;
    printf("Куб суми прних чисел = %d\n", cubesum);

    return 0;
}







#include <stdio.h>

int main() {
    int sum = 0, i = 11;
    while (i <= 20) {
        if (i % 2 == 0) 
        sum += i;
        i++;
    }
    int cubesum = sum * sum * sum;
    printf("Куб суми прних чисел = %d\n", cubesum);

    return 0;
}







#include <stdio.h>

int main() {
    int sum = 0, i = 11;
    do {
        if (i % 2 == 0) 
        sum += i;
        i++;
    } while (i <= 20);
    int cubesum = sum * sum * sum;
    printf("Куб суми прних чисел = %d\n", cubesum);

    return 0;
}



